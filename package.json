{
  "name": "bun-hono-server",
  "version": "1.1.0",
  "description": "A fast and modern web server template built with Bun and Hono",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "build": "bun build src/index.ts --outfile=dist/index.js --target=bun",
    "start": "bun run dist/index.js",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "script:admin": "bun run src/scripts/create-admin.ts",
    "docs:generate": "bun run scripts/generate-openapi.ts",
    "docs:watch": "bun run scripts/generate-openapi.ts --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.884.0",
    "@aws-sdk/protocol-http": "^3.374.0",
    "@aws-sdk/s3-request-presigner": "^3.884.0",
    "@hono/standard-validator": "^0.1.5",
    "@hono/zod-validator": "^0.2.0",
    "@types/sharp": "^0.32.0",
    "bcryptjs": "^2.4.3",
    "cos-nodejs-sdk-v5": "^2.15.4",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.30.0",
    "hono": "^4.9.4",
    "hono-openapi": "^0.4.8",
    "hono-rate-limiter": "^0.4.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.9.0",
    "pino-roll": "^3.1.0",
    "zod": "^3.22.0",
    "zod-openapi": "^5.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bun": "latest",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "drizzle-kit": "^0.20.0",
    "pino-pretty": "^13.1.1",
    "ts-morph": "^23.0.0",
    "typescript": "^5.5.4",
    "zod-to-json-schema": "^3.23.0"
  }
}